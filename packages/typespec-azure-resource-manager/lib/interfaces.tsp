  Properties extends TypeSpec.Reflection.Model
>
  extends ExtensionResourceRead<Resource>,
    ExtensionResourceCreate<Resource>,
    ExtensionResourceUpdate<Resource, Properties>,
    ExtensionResourceDelete<Resource> {}

