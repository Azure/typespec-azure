{
  "name": "@azure-tools/typespec-diff",
  "version": "0.37.0",
  "author": "Microsoft Corporation",
  "description": "TypeSpec specs diff",
  "homepage": "https://azure.github.io/typespec-azure",
  "readme": "https://github.com/Microsoft/typespec/blob/main/README.md",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Azure/typespec-azure.git"
  },
  "bugs": {
    "url": "https://github.com/Azure/typespec-azure/issues"
  },
  "keywords": [
    "typespec",
    "dpg"
  ],
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/src/index.js"
    },
    "./testing": {
      "types": "./dist/src/testing/index.d.ts",
      "default": "./dist/src/testing/index.js"
    }
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "clean": "rimraf ./dist ./temp",
    "build": "tsc -p . ",
    "watch": "tsc -p . --watch",
    "lint-typespec-library": "tsp compile . --warn-as-error --import @typespec/library-linter --no-emit",
    "test": "mocha",
    "test-official": "c8 mocha --forbid-only --reporter mocha-multi-reporters",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint:fix": "eslint . --fix --ext .ts"
  },
  "files": [
    "lib/*.tsp",
    "dist/**",
    "!dist/test/**"
  ],
  "bin": {
    "typespec-diff": "dist/src/index.js"
  },
  "peerDependencies": {
    "@typespec/compiler": "workspace:~0.51.0",
    "@typespec/rest": "workspace:~0.51.0",
    "@typespec/http": "workspace:~0.51.0",
    "@typespec/openapi": "workspace:~0.51.0",
    "@typespec/versioning": "workspace:~0.51.0"
  },
  "dependencies": {
    "yargs": "~17.7.2",
    "lodash": "~4.17.21",
    "ajv": "~8.12.0",
    "js-yaml": "~4.1.0",
    "picocolors": "~1.0.0"
  },
  "devDependencies": {
    "@types/yargs": "~17.0.32",
    "@types/mocha": "~10.0.6",
    "@types/lodash": "4.14.191",
    "@types/js-yaml": "~4.0.1",
    "@types/node": "~18.11.9",
    "@typespec/eslint-config-typespec": "workspace:~0.51.0",
    "@typespec/library-linter": "workspace:~0.51.0",
    "@typespec/eslint-plugin": "workspace:~0.51.0",
    "@azure-tools/typespec-azure-resource-manager": "workspace:~0.37.1",
    "@azure-tools/typespec-azure-core": "workspace:~0.37.2",
    "eslint": "^8.55.0",
    "mocha": "~10.2.0",
    "mocha-junit-reporter": "~2.2.1",
    "mocha-multi-reporters": "~1.5.1",
    "c8": "~8.0.1",
    "rimraf": "~5.0.1",
    "typescript": "~5.3.3"
  }
}
