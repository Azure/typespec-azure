    "@docusaurus/theme-common": "~3.2.0",
    "@fluentui/react-components": "~9.47.2",
    "@mdx-js/react": "^3.0.1",
    "@typespec/playground": "workspace:~",
    "@typespec/website": "workspace:~",
    "clsx": "^2.1.0",
    "es-module-shims": "~1.9.0",
    "prism-react-renderer": "^2.3.1",
    "prismjs": "~1.29.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0"
  },
  "devDependencies": {
    "@azure-tools/typespec-azure-core": "workspace:~",
    "@azure-tools/typespec-azure-playground-website": "workspace:~",
